  "main": "configurations/core.js",
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "find-rules": "eslint-find-rules --unused ./configurations/all.js",
    "print-config": "eslint --print-config index.js > config-full.json",
