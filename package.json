    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "eslint-plugin-unicorn": "^26.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "np": "^7.2.0",
    "svelte": "^3.31.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
