    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "np": "^6.5.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "eslint": "^7.3.0"
